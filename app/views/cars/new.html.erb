<h1>New Car</h1>

<%= form_for @car do |f| %>
  <div>
    <%= f.label :make %>
    <%= f.text_field :make %>
  </div>

  <div>
    <%= f.label :model %>
    <%= f.text_field :model %>
  </div>

  <div>
    <%= f.label :year %>
    <%= f.number_field :year %>
  </div>


  <div>
    <%= f.label :fuel_type %>
    <%= f.text_field :fuel_type %>
  </div>
<!--  <div>-->
    <%#= f.label :user_id %>
    <%#= f.collection_select(:user_id, User.all, :id, :make) %>
<!--  </div>-->

  <div>
    <%= f.submit %>
  </div>
<% end %>

<%= link_to 'Back', cars_path %>
