<h1>Edit <%= @car.make %> <%= @car.model %></h1>

<%= form_for @car do |f| %>
  <div>
    <%= f.label :make %>
    <%= f.text_field :make %>
  </div>

  <div>
    <%= f.label :model %>
    <%= f.text_field :model %>
  </div>

  <div>
    <%= f.label :year %>
    <%= f.number_field :year %>
  </div>

  <div>
    <%= f.label :user_id %>
    <%= f.collection_select(:user_id, User.all, :id, :name) %>
  </div>

  <div>
    <%= f.submit %>
  </div>
<% end %>

<%= link_to 'Cancel', car_path(@car) %>
